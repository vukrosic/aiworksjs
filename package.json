{
  "name": "openaicodexplayground",
  "displayName": "OpenAI Codex",
  "description": "OpenAI Playground functionality in your editor.",
  "icon": "READMEImages/OpenAILogo.jpg",
  "publisher": "VukRosic",
  "repository": {
    "type": "git",
    "url": "https://github.com/vukrosic/aiworksjs"
  },
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:aiworksjs.wtffff",
    "onCommand:aiworksjs.setAttributes",
    "onCommand:aiworksjs.setAPIKey",
    "onCommand:aiworksjs.setModel",
    "onCommand:aiworksjs.setTemperature",
    "onCommand:aiworksjs.submitExtensionFeedback"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiworksjs.wtffff",
        "title": "OpenAI Codex: Call Codex API"
      },
      {
        "command": "aiworksjs.setAttributes",
        "title": "OpenAI Codex: Set Max Length"
      },
      {
        "command": "aiworksjs.setAPIKey",
        "title": "OpenAI Codex: Set API Key"
      },
      {
        "command": "aiworksjs.setModel",
        "title": "OpenAI Codex: Set Model"
      },
      {
        "command": "aiworksjs.setTemperature",
        "title": "OpenAI Codex: Set Temperature"
      },
      {
        "command": "aiworksjs.submitExtensionFeedback",
        "title": "OpenAI Codex: Submit Extension Feedback"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.73.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "js-yaml": "^4.1.0",
    "openai": "^3.1.0",
    "xmlhttprequest": "^1.8.0"
  }
}
